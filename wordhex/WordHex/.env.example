# Enable source maps for easier debugging
NODE_OPTIONS="--enable-source-maps"

# Discord credentials (from https://discord.com/developers/applications)
DISCORD_CLIENT_ID="your_application_id"
DISCORD_CLIENT_SECRET="your_client_secret"
VITE_DISCORD_CLIENT_ID="your_application_id"
# Redirect URI must match the proxy URL in your Activity mapping
DISCORD_REDIRECT_URI="https://your-tunnel-url/api/token"

# The port the server will listen on locally
PORT="3000"
